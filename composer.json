{
    "name": "webgriffe/amp-csv",
    "description": "CSV library to use with Amp PHP framework.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Webgriffe SRL",
            "email": "support@webgriffe.com"
        }
    ],
    "require": {
        "php": ">=7",
        "amphp/amp": "^2.0",
        "amphp/file": "^0.3.0"
    },
    "autoload": {
        "psr-4": {"Webgriffe\\AmpCsv\\": "src/"}
    },
    "autoload-dev": {
        "psr-4": {"Webgriffe\\AmpCsv\\": "tests/"}
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "amphp/phpunit-util": "^1.0",
        "squizlabs/php_codesniffer": "^3.2"
    }
}
